{
    "name": "generationtux/healthz",
    "description": "Health checks for PHP apps.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Ferguson",
            "email": "kyle.ferguson@generationtux.com"
        },
        {
          "name": "Damien Russell",
          "email": "christopher.russell@generationtux.com"
        },
        {
          "name": "Thomas Manley",
          "email": "thomas.manley@generationtux.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Gentux\\Healthz\\": "src/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6.2|^7.0.1",
        "aws/aws-sdk-php": "~3.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "illuminate/queue": "^7.0|^8.0",
        "illuminate/console": "^7.0|^8.0",
        "twig/twig": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "^1.2.1",
        "gecko-packages/gecko-memcache-mock": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gentux\\Healthz\\Support\\HealthzServiceProvider"
            ]
        }
    }
}
